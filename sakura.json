{
    "version": "2.4.2",
    "description": "Sakura Editor is a Japanese text editor.",
    "homepage": "https://sakura-editor.github.io/",
    "license": "zlib",
    "architecture": {
        "32bit": {
            "url": "https://github.com/sakura-editor/sakura/releases/download/v$version/sakura-tag-v$version-build4203-a3e63915b-Win32-Release-Exe.zip",
            "hash": "auto"
        }
    },
    "bin": "sakura.exe",
    "installer": {
        "script": [
            "Expand-Archive \"$dir\\sakura-tag-v$version-build4203-a3e63915b-Win32-Release-Exe.zip\" -DestinationPath \"$dir\""
        ]
    },
    "checkver": {
        "github": "https://github.com/sakura-editor/sakura",
        "regex": "releases/tag/v([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/sakura-editor/sakura/releases/download/v$version/sakura-tag-v$version-build4203-a3e63915b-Win32-Release-Exe.zip",
        "hash": {
            "url": "$url.md5"
        },
        "regex": "sakura-tag-v$version-build([\\d]+)-([\\w]+)-Win32-Release-Exe.zip"
    }
}
